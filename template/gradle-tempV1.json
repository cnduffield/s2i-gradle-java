{
   "kind": "ImageStream",
   "apiVersion": "image.openshift.io/v1",
   "metadata": {
      "name": "s2i-java-gradle-v1",
      "namespace": "openshift"
   },
   "spec": {
"dockerImageRepository": "quay.io/repository/cduffiel/myrepo",
      "tags": [
         {
            "name": "ubi",
            "annotations": {
               "description": "Java Gradle OpenJDK S2I image",
               "iconClass": "icon-jboss",
               "tags": "builder,java,gradle,springboot,dropwizard,microservices,fatjar",
               "supports": "java:8,gradle:5.6",
               "sampleRepo": "https://github.com/cnduffield/gradlespring.git",
               "version": "ubi"
            }
         }
      ]
   }
}
